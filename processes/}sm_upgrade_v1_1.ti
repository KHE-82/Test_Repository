CELLPUTS( 'N', '}sm_control_attribute_settings', vAttribute, 'Task Action', 'read_only');
CELLPUTS( 'List Value', '}sm_control_attribute_settings', vAttribute, 'Task Action', 'list_dim_alias');

endif;
